{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "pm",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "pm",
            "kebab-case"
        ],
		"indent": [
			true,
			"tabs"
		],
		"no-inferrable-types": [
			false
		]
    }
}
